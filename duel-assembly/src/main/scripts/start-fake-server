#/bin/bash

SCRIPT_DIR=$(cd $(dirname "${BASH_SOURCE[0]}") && pwd)
DUEL_HOME=$(dirname "$SCRIPT_DIR")

if [ -n "$JAVA_HOME" ]; then
  COMMAND="$JAVA_HOME/bin/java"
else
  COMMAND=java
fi

MODULE=org.leadpony.duel.fake.server/org.leadpony.duel.fake.server.FakeServer
"${COMMAND}" -p "${DUEL_HOME}/lib" -m ${MODULE} start
